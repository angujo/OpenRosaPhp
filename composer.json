{
  "name": "angujo/openrosaphp",
  "description": "Php version of OpenRosa",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "beta",
  "require": {
    "php": "^7.0",
    "adbario/php-dot-notation": "^2.1",
    "fzaninotto/faker": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Angujo\\OpenRosaPhp\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7"
  },
  "scripts": {
    "post-root-package-install": [
      "chmod -R 777 vendor/angujo/openrosaphp/src/temp"
    ],
    "post-install-cmd": [
      "chmod -R 777 vendor/angujo/openrosaphp/src/temp"
    ],
    "post-update-cmd": [
      "chmod -R 777 vendor/angujo/openrosaphp/src/temp"
    ]
  }
}
